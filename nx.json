{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "install",
          "list"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "install": {
      "dependsOn": [
        "^install"
      ],
      "outputs": [
        "{projectRoot}/prebuilds"
      ]
    },
    "prepublishOnly": {
      "dependsOn": [
        "^prepublishOnly"
      ]
    },
    "list": {
      "dependsOn": [
        "^list"
      ]
    }
  }
}
