{
    "name": "rev-hub-cli",
    "descripton": "sample for testing Rev Hub code",
    "version": "1.0.0",
    "main": "dist/main.js",
    "bin": "revhub",
    "private": true,
    "type": "module",
    "dependencies": {
        "@rev-robotics/control-hub": "1.0.0",
        "@rev-robotics/expansion-hub": "1.0.0",
        "ws": "8.13.0",
        "commander": "^10.0.1",
        "@u4/adbkit": "^4.1.19",
        "get-port": "^6.1.2"
    },
    "devDependencies": {
        "@types/ws": "^8.5.4",
        "@types/node-forge": "^1.3.2",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-typescript": "^11.1.1",
        "@rollup/plugin-commonjs": "^25.0.1",
        "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
        "@rollup/plugin-json": "^6.0.0",
        "rollup-plugin-natives": "^0.7.6",
        "@types/debug": "^4.1.8"
    },
    "scripts": {
        "build": "tsc",
        "motor": "node dist/main.js motor power 1 0.60",
        "analog": "node dist/main.js analog 0",
        "battery": "node dist/main.js battery --continuous",
        "voltage": "node dist/main.js voltage --continuous",
        "temperature": "node dist/main.js temperature --continuous",
        "error": "node dist/main.js testErrorHandling",
        "list": "node dist/main.js list",
        "led": "node dist/main.js led",
        "motor": "node dist/main.js motor power 1 0.60",
        "motor-position": "node dist/main.js --count 3000",
        "distance": "node dist/main.js distance 0"
    }
}
